{
  "name": "fontfacegen",
  "description": "Browser font-face generator for creating browser usable fonts from ttf's or otf's",
  "version": "0.4.0",
  "homepage": "https://github.com/agentk/fontfacegen",
  "author": {
    "name": "Karl Bowden",
    "email": "karlbowden@gmail.com",
    "url": "http://karlbowden.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/agentk/fontfacegen.git"
  },
  "bugs": {
    "url": "https://github.com/agentk/fontfacegen/issues"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "lint": "eslint '**/*.js'",
    "pretest": "npm run lint",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build",
    "test": "node -r @babel/register test.js"
  },
  "main": "index.js",
  "keywords": [
    "font-face",
    "fontface",
    "font",
    "face",
    "web-fonts",
    "webfonts",
    "web",
    "ttf",
    "otf",
    "svg",
    "eot",
    "css"
  ],
  "dependencies": {
    "mkdirp": "^0.5.1",
    "ttf2eot": "^2.0.0",
    "ttf2svg": "^1.2.0",
    "ttf2woff2": "^3.0.0",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.17.10",
    "@babel/register": "^7.7.4",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1"
  }
}
